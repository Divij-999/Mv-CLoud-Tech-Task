<apex:page docType="html-5.0" controller="LoginForm">
    <apex:pageBlock title="List of Accounts">
    	<apex:pageBlockTable value="{!Accounts}" var="a">
            <apex:column value="{!a.Name}"/>
            <apex:column value="{!a.Type}"/>
            <apex:column value="{!a.Phone}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>